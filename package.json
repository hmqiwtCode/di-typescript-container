{
  "name": "di-typescript-container",
  "version": "1.0.0",
  "description": "A powerful and flexible TypeScript dependency injection container",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
