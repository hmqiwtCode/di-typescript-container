  "bugs": {
    "url": "https://github.com/hmqiwtCode/di-typescript-container/issues"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.23",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "rimraf": "^5.0.5",
    "eslint": "^8.54.0",
